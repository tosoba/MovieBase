target 'MovieBase' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  # ignore all warnings from all pods
  inhibit_all_warnings!
  
  # DI
  pod 'Swinject'
  pod 'SwinjectStoryboard'
  
  # Network
  pod 'Himotoki'
  pod 'Alamofire'
  
  # Rx
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxOptional'

end

